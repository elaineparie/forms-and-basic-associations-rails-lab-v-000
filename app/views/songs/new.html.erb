<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :id, :name%>
  <input name="song[notes_ids][]" id="song_notes_1">
  <input name="song[notes_ids][]" id="song_notes_2">
  <input name="song[notes_ids][]" id="song_notes_3">

  <br />

  <%= f.submit %>
<% end %>
